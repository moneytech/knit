#testing statements executed at global scope

max = 10;
primes = [];
i = 0;
while (i < max) {
    primes.append(true);
    i = i + 1;
}
while ((i < max) and (i <= 1)) {
    primes[i] = false;
    i = i + 1;
}
i = 1;
while (i < max) {
    if (primes[i]) {
        j = i * 2;
        while (j < max) {
            primes[j] = false;
            j = j + i;
        }
    }
    i = i + 1;
}
i = 0;
while (i < max) {
    if (primes[i]) {
        print(i);
    }
    i = i + 1;
}
